{
  "apiUrl": "https://api.mlab.com/api/1/databases/toolkitaxa/collections/{path}",
  "baseUrl" : "/react-toolkit/latest/demo/",
  "oidc": {
    "isEnabled": false,
    "configuration": {
      "client_id": "#{Oidc:ClientId}#",
      "redirect_uri": "#{Oidc:RedirectUri}#",
      "response_type": "code",
