mixin ButtonLink({ className='btn', label, icon='', target="_self", modifier='', js=false, href})
    -const classComponent = `${setClass(className, modifier, js)}`
    a(class=classComponent target=target href=href)
        if(icon !== '')
            +Icon({id:icon, className:`${prefix}-btn__icon`})
        span(class=`${prefix}-btn__text`) #{label}