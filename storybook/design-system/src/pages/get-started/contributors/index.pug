extends ../../../commons/layout

block vars
  - var id = "getstarted"

block meta
  -var activePath = '/get-started/contributors'
  -const { metatitle, metadescription, metakeywords } = require(`../src/pages${activePath}/meta.json`)
  -var metaTitle = metatitle
  -var metaDescription = metadescription
  -var metaKeywords = metakeywords
  -var basedir='../../'

block menu
  +MenuGetstarted({ activePath })

block headerlinks
  +Headerlinks({ activePath })

block page
  +PageTitle({ title: 'Contributeurs' })
  +PageContent()
    include ./templates/contributors-page.pug
