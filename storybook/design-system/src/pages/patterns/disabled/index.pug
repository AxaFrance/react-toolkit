extends ../../../commons/layout

block meta
  -const { metatitle, metadescription, metakeywords } = require('../src/pages/patterns/disabled/meta.json')
  -var metaTitle = metatitle
  -var metaDescription = metadescription
  -var metaKeywords = metakeywords
  -var basedir='../../'

block menu
  +MenuPatterns({ activePath:'/patterns/disabled' })

block headerlinks
  +Headerlinks({ activePath:'/patterns' })

block page
  +PageTitle({ title: 'Désactivé' })
  +PageContent()
    +PageMarkdown()
      include:markdown-it(html) ./markdown/content.md
